{
  "name": "shadcn-create-app.playlexi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:speech": "npx ts-node speech-server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:speech\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx wrangler d1 migrations apply playlexi-db --local",
    "db:migrate:prod": "npx wrangler d1 migrations apply playlexi-db --remote",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx ts-node --project scripts/tsconfig.json scripts/seed-words.ts",
    "db:seed:prod": "npx ts-node --project scripts/tsconfig.json scripts/seed-words.ts --production",
    "db:seed:dry": "npx ts-node --project scripts/tsconfig.json scripts/seed-words.ts --dry-run",
    "tts:generate": "npx ts-node --project scripts/tsconfig.json scripts/generate-tts.ts",
    "tts:generate:prod": "npx ts-node --project scripts/tsconfig.json scripts/generate-tts.ts --production",
    "tts:generate:dry": "npx ts-node --project scripts/tsconfig.json scripts/generate-tts.ts --dry-run",
    "tts:estimate": "npx ts-node --project scripts/tsconfig.json scripts/generate-tts.ts --estimate"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@google-cloud/speech": "^7.2.1",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "motion": "^12.23.26",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "nucleo-core-outline-24": "^1.1.3",
    "radix-ui": "^1.4.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "shadcn": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
    "@cloudflare/workers-types": "^4.20260117.0",
    "@opennextjs/cloudflare": "^1.14.9",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.2"
  }
}
